CC=CC
OBJS=main.o sort.o array_funcs.o malloc_1.o array.o array1.o char_array.o bisearch.o bit32_64.o pointer1.o struct1.o memory_1.o arrlist.o linklist1.o stuck1.o linkqueue1.o arrqueue1.o arrstuck1.o arrbtree1.o recursion.o hanoi.o basefuncs.o base_math.o fork1.o base_exec.o -lm
HEADPATH=./headfiles/
EXEC=main

all: cleanall ${EXEC}

$(EXEC): ${OBJS}
	${CC} ${OBJS} -o ${EXEC}

main.o: main.c  
	${CC} -c -g main.c -I ${HEADPATH} -o $@
sort.o:	sort/sort.c
	${CC} -c -g sort/sort.c -I ${HEADPATH} -o $@


array_funcs.o: ArrayPoint/array_funcs.c
	${CC} -c -g ArrayPoint/array_funcs.c -I ${HEADPATH} -o $@

malloc_1.o: ArrayPoint/malloc_1.c
	${CC} -c -g ArrayPoint/malloc_1.c -I ${HEADPATH} -o $@

array.o: ArrayPoint/array.c
	${CC} -c -g ArrayPoint/array.c -I ${HEADPATH} -o $@

array1.o: Data_struct/array1.c
	${CC} -c -g Data_struct/array1.c -I ${HEADPATH} -o $@

char_array.o: char/char_array.c
	${CC} -c -g char/char_array.c -I ${HEADPATH} -o $@

bisearch.o: bisearch/bisearch.c
	${CC} -c -g bisearch/bisearch.c -I ${HEADPATH} -o $@

bit32_64.o: bit/bit32_64.c
	${CC} -c -g bit/bit32_64.c -I ${HEADPATH} -o $@

pointer1.o: Data_struct/pointer1.c
	${CC} -c -g Data_struct/pointer1.c -I ${HEADPATH} -o $@

struct1.o: Data_struct/struct1.c
	${CC} -c -g Data_struct/struct1.c -I ${HEADPATH} -o $@

memory_1.o: Memory/memory_1.c
	${CC} -c -g Memory/memory_1.c -I ${HEADPATH} -o $@

arrlist.o: Data_struct/arrlist.c
	${CC} -c -g Data_struct/arrlist.c -I ${HEADPATH} -o $@

linklist1.o: Data_struct/linklist1.c
	${CC} -c -g Data_struct/linklist1.c -I ${HEADPATH} -o $@

stuck1.o: Data_struct/stuck1.c
	${CC} -c -g Data_struct/stuck1.c -I ${HEADPATH} -o $@
	
linkqueue1.o: Data_struct/linkqueue1.c
	${CC} -c -g Data_struct/linkqueue1.c -I ${HEADPATH} -o $@
	
arrqueue1.o: Data_struct/arrqueue1.c
	${CC} -c -g Data_struct/arrqueue1.c -I ${HEADPATH} -o $@
	
arrstuck1.o: Data_struct/arrstuck1.c
	${CC} -c -g Data_struct/arrstuck1.c -I ${HEADPATH} -o $@
	
arrbtree1.o: Data_struct/arrbtree1.c
	${CC} -c -g Data_struct/arrbtree1.c -I ${HEADPATH} -o $@
	
recursion.o: Data_struct/Recursion/recursion.c
	${CC} -c -g Data_struct/Recursion/recursion.c -I ${HEADPATH} -o $@
	
hanoi.o: Data_struct/Recursion/hanoi.c
	${CC} -c -g Data_struct/Recursion/hanoi.c -I ${HEADPATH} -o $@
	
basefuncs.o: Basefuncs/basefuncs.c
	${CC} -c -g Basefuncs/basefuncs.c -I ${HEADPATH} -o $@

base_math.o: Basefuncs/base_math.c
	${CC} -c -g Basefuncs/base_math.c -I ${HEADPATH} -o $@

fork1.o: Process/fork1.c
	${CC} -c -g Process/fork1.c -I ${HEADPATH} -o $@
	
base_exec.o: Basefuncs/base_exec.c
	${CC} -c -g Basefuncs/base_exec.c -I ${HEADPATH} -o $@
	
cleanall:
	rm -rf ${EXEC} *.o

clean:
	rm -rf *o


###  $< ==> means the first one of the objs which are depended ex main.c of line 12
###  $@ ==> means the current obj
###  $^ ==> means all the objs which are depended
